/**
 * Cursors classes
 */


/**
 * Render cursors classes
 *
 * @access public
 */
util-render-cursors()

    /.cursor-pointer
        cursor: pointer

    /.cursor-move
        cursor: move

    /.cursor-default
        cursor: default

    /.cursor-grab
        cursor: move
        cursor: -webkit-grab
        cursor: grab

